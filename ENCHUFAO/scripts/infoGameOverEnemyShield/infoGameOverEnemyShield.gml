function infoGameOverEnemyShield(argument0)
{
	switch(argument0)
	{
		case 0: text = "Don't touch them"; break;
		case 1: text = "They use a shield now!?"; break;
		case 2: text = "You are faster than them"; break;
		case 3: text = "Shoot'em up"; break;
		case 4: text = "Do not get surrounded"; break;
		case 5: text = "Attack them from behind"; break;
		case 6: text = "Use a bomb next time"; break;
		case 7: text = "Shields are weak against laser weapons"; break;
		case 8: text = "Enemies take cover behind them"; break;
	}
}