function infoGameOverEnemyBombs(argument0)
{
	switch(argument0)
	{
		case 0: text = "Yes, they shoot bombs"; break;
		case 1: text = "Don't touch them. And don't touch the bombs"; break;
		case 2: text = "Boooooomb"; break;
		case 3: text = "Shoot'em up"; break;
		case 4: text = "You can destroy their bombs"; break;
		case 5: text = "You should have shot first"; break;
		case 6: text = "Ok, lets bomb THEM next time"; break;
		case 7: text = "Be careful with their bombs"; break;
	}
}