function infoGameOverEnemyMini(argument0)
{
	switch(argument0)
	{
		case 0: text = "Don't touch them"; break;
		case 1: text = "They are weak"; break;
		case 2: text = "Be faster than them"; break;
		case 3: text = "Shoot'em up"; break;
		case 4: text = "Do not get surrounded"; break;
		case 5: text = "Too many can be a problem"; break;
		case 6: text = "Keep an eye on their mothership"; break;
		case 7: text = "Very weak against laser weapons"; break;
		case 8: text = "They attack in numbers"; break;
	}
}