function infoGameOverEnemyWorm(argument0)
{
	switch(argument0)
	{
		case 0: text = "Space worms!?"; break;
		case 1: text = "They are free"; break;
		case 2: text = "Don't touch them"; break;
		case 3: text = "Some specimens can plug to the spacesockets"; break;
		case 4: text = "Longer = Deadlier"; break;
		case 5: text = "Do not get surrounded by them"; break;
		case 6: text = "There's always a bigger one"; break;
		case 7: text = "No brain, only mouth"; break;
		case 8: text = "You can burn every segment"; break;
		case 9: text = "You can damage them in every semgent"; break;
		case 10: text = "A bomb seems like a good idea again"; break;
	}
}