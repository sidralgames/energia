// Script assets have changed for v2.3.0 see
// https://help.yoyogames.com/hc/en-us/articles/360005277377 for more information
function StrandedShip_False()
{
	if (sprite = splayer)
	{
		global.strandedShipGreen = false;
	}
	else if (sprite = splayer_Blue)
	{
		global.strandedShipBlue = false;
	}
	else if (sprite = splayer_Dark)
	{
		global.strandedShipDark = false;
	}
	else if (sprite = splayer_Red)
	{
		global.strandedShipRed = false;
	}
	else if (sprite = splayer_Purple)
	{
		global.strandedShipPurple = false;
	}
}