function infoGameOverEnemyWall(argument0)
{
	switch(argument0)
	{
		case 0: text = "Don't get close to walls"; break;
		case 1: text = "Ups, that plant had a MOUTH"; break;
		case 2: text = "Plants don't move. It was an enemy."; break;
		case 3: text = "Stay out of their range"; break;
		case 4: text = "Longer = Deadlier"; break;
		case 5: text = "Inspect the walls"; break;
		case 6: text = "They camouflage among vegetation"; break;
		case 7: text = "No brain, only mouth"; break;
		case 8: text = "Shoot them on sight"; break;
		case 9: text = "Clear the walls"; break;
		case 10: text = "Burn them"; break;
	}
}